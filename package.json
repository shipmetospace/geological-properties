    "workerDirectory": "public"
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "yarn lint",
      "bash -c 'yarn check-types'"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "storybook": "9",
        "tailwindcss": "4",
        "eslint": "9",
        "react": "19"
      }
    }
  }
